(jbuild_version 1)

(ocamllex (lexer))
(ocamlyacc (parser))

(library
 ((name           mincaml)
  (modules        (:standard \ main anchor))
  (libraries      (str core util))
  (c_names        (float))
  (preprocess
   (pps (ppx_deriving.show)))
))

(executable
 ((name           main)
  (modules        Main)
  (libraries      (mincaml))
))
